{
  "cli": {
    "version": ">= 5.4.0",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "ios": {
        "image": "macos-ventura-13.6-xcode-15.2"
      },
      "channel": "production",
      "node": "18.18.0",
      "autoIncrement": true,
      "env": {}
    },
    "ossProduction": {
      "extends": "production",
      "autoIncrement": false,
      "android": {
        "buildType": "apk",
        "resourceClass": "large"
      },
      "env": {}
    },
    "tenantPreviewer": {
      "extends": "production",
      "distribution": "store",
      "autoIncrement": true
    },
    "development": {
      "extends": "production",
      "autoIncrement": false,
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "extends": "production",
      "autoIncrement": false,
      "channel": "preview",
      "distribution": "internal"
    },
    "demo": {
      "extends": "production",
      "autoIncrement": false,
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "961771928"
      }
    },
    "tenantPreviewer": {
      "ios": {
        "ascAppId": "6473803640"
      }
    }
  }
}
